<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netNamedPipeBinding>
                <binding name="NetNamedPipeBinding_ICorsairHydroService">
                    <security mode="None" />
                </binding>
                <binding name="NetNamedPipeBinding_IHardwareMonitorService">
                    <security mode="None" />
                </binding>
            </netNamedPipeBinding>
        </bindings>
        <client>
            <endpoint address="net.pipe://localhost/CorsairHydroService"
                binding="netNamedPipeBinding" bindingConfiguration="NetNamedPipeBinding_ICorsairHydroService"
                contract="HydroService.ICorsairHydroService" name="NetNamedPipeBinding_ICorsairHydroService" />
            <endpoint address="net.pipe://localhost/CorsairHydroHardwareMonitorService"
                binding="netNamedPipeBinding" bindingConfiguration="NetNamedPipeBinding_IHardwareMonitorService"
                contract="HardwareMonitorService.IHardwareMonitorService"
                name="NetNamedPipeBinding_IHardwareMonitorService" />
        </client>
    </system.serviceModel>
</configuration>